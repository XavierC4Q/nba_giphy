{
    "presets": [
     ["@babel/preset-env", {
        "useBuiltIns": "entry"
      }],
     "@babel/preset-react"
    ],
    "plugins": [
        "transform-class-properties",
        [
            "@babel/plugin-transform-runtime",
            {
              "corejs": false,
              "helpers": true,
              "regenerator": true,
              "useESModules": false
            }
          ]
    ]   
}